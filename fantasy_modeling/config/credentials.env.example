# ESPN Fantasy Basketball Authentication
# Copy this to credentials.env and fill in your values

# ESPN cookies (get from browser dev tools when logged in)
ESPN_SWID={YOUR_SWID_HERE}
ESPN_S2={YOUR_ESPN_S2_HERE}

# Optional: Database connection
DB_PATH=./data/fantasy_basketball.db

# Optional: Cache settings
CACHE_DIR=./cache
CACHE_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/fantasy_modeling.log